{
  "name": "@inst-pkg/cli",
  "version": "2.0.0-beta",
  "description": "Instantly create web and native applications for React with Serverless SSR out of the box",
  "main": "cjs/index.js",
  "bin": {
    "inst": "cjs/index.js"
  },
  "scripts": {
    "edit": "atom ./",
    "clean": "rimraf ./dist/cjs",
    "build": "yarn clean && cross-env NODE_ENV=production BABEL_ENV=production babel src --out-dir dist/cjs && npm run prettier \"dist/cjs/**/*.js\"",
    "prettier": "prettier --single-quote --no-semi --no-bracket-spacing --trailing-comma es5 --write",
    "cli": "node dist/cjs/index.js"
  },
  "repository": "https://github.com/jaredLunde/inst",
  "author": "Jared Lunde",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@inst-app/babel-preset-esx": "^1.0.2",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "@inst-pkg/basic-template": "^1.0.0",
    "@inst-pkg/template-utils": "^1.0.1",
    "@yarnpkg/lockfile": "^1.1.0",
    "chalk": "^2.4.1",
    "fs-extra": "^7.0.0",
    "import-from": "^2.1.0",
    "inquirer": "^6.1.0",
    "klaw-sync": "^4.0.0",
    "memoize-two-args": "^1.0.7",
    "ncp": "^2.0.0",
    "ora": "^3.0.0",
    "rimraf": "^2.6.2",
    "yargs": "^12.0.5"
  }
}
